{
  "env": {
    "browser": true,
    "es2021": true
  },
  "globals": {
    "bootstrap": "readonly"
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-undef": "error"
  }
}
